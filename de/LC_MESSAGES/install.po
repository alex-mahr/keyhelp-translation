msgstr "Serverdaten"

#: install/install.php:361 install/install.php:393
msgid "Server domain (FQDN)"
msgstr "Server Domain (FQDN)"

#: install/install.php:363
msgid "Administrator account"
msgstr "Administrator Konto"

#: install/install.php:364
msgid "Username"
msgstr "Benutzername"

#: install/install.php:365 install/install.php:849 install/install.php:852
msgid "Password"
msgstr "Passwort"

#: install/install.php:366
msgid "Email address"
msgstr "E-Mail Adresse"

#: install/install.php:368
msgid "Misc"
msgstr "Sonstiges"

#: install/install.php:369
msgid "Notification after finishing"
msgstr "Benachrichtigung nach Fertigstellung"

#: install/install.php:369
msgid "No"
msgstr "Nein"

